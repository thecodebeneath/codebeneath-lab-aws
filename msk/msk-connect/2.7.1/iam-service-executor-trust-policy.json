{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Principal": {
				"Service": "kafkaconnect.amazonaws.com"
			},
			"Action": "sts:AssumeRole",
			"Condition": {
				"StringEquals": {
					"aws:SourceAccount": "732457136693"
				},
				"ArnLike": {
					"aws:SourceArn": "arn:aws:kafkaconnect:us-east-2:732457136693:connector/*"
				}
			}
		}
	]
}